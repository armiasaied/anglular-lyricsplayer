{
  "name": "ng-lyrics-player",
  "version": "1.0.0",
  "description": "Angular directive for SoundCloud lyrics player",
  "main": "index.js",
  "scripts": {
    "browserify": "browserify src/ng-lyrics-player.js --standalone=ng-lyrics-player -o dist/ng-lyrics-player.js",
    "uglify": "npm run browserify && uglifyjs dist/ng-lyrics-player.js -o dist/ng-lyrics-player.min.js",
    "release": "npm run uglify",
    "bump": "node scripts/bump",
    "serve": "http-server -p 8000",
    "start": "npm run serve"
  },
  "dependencies": {
    "audio-player": "^1.0.0",
    "hhmmss": "^1.0.0",
    "soundcloud-resolve-jsonp": "^1.3.0"
  },
  "devDependencies": {
    "blkmark": "^1.0.1",
    "browserify": "^9.0.3",
    "highlight.js": "^8.4.0",
    "http-server": "^0.7.5",
    "lodash": ">=4.17.11",
    "markdown-toc": "^0.11.0",
    "marked": "^0.3.3",
    "uglify-js": "^2.4.17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/armiasaied/ng-lyrics-player.git"
  },
  "author": "Armia Saied",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/armiasaied/ng-lyrics-playe/issues"
  },
  "homepage": "https://github.com/armiasaied/ng-lyrics-player"
}
